{
	"name": "vue-next-heroicons",
	"version": "1.0.2",
	"homepage": "https://github.com/dapotatoman/vue-next-heroicons",
	"repository": {
		"url": "dapotatoman/vue-next-heroicons",
		"type": "git"
	},
	"license": "MIT",
	"description": "Hero Icons library component, made for Vue 3",
	"keywords": [
		"vue",
		"vue3",
		"vuejs",
		"icons",
		"hero-icons",
		"hero icons"
	],
	"author": {
		"name": "Fahadul Islam"
	},
	"sideEffects": "false",
	"scripts": {
		"serve": "vue-cli-service serve dev/serve.js",
		"lint:fix": "eslint --fix src/**",
		"generate": "node ./build/generate.js",
		"package": "node ./build/package.js",
		"build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && node ./build/package.js",
		"build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
		"build:cjs": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
		"build:min": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@rollup/plugin-alias": "^3.1.1",
		"@rollup/plugin-babel": "^5.2.1",
		"@rollup/plugin-commonjs": "^16.0.0",
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@rollup/plugin-replace": "^2.3.4",
		"@vue/babel-plugin-jsx": "^1.0.0-rc.3",
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"@vue/compiler-sfc": "^3.0.0",
		"@vue/eslint-config-airbnb": "^5.0.2",
		"babel-eslint": "^10.1.0",
		"core-js": "^3.6.5",
		"cross-env": "^7.0.2",
		"eslint": "^6.7.2",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-vue": "^7.0.0-0",
		"fs-extra": "^9.0.1",
		"heroicons": "^0.4.2",
		"minimist": "^1.2.5",
		"pascal-case": "^3.1.1",
		"rollup": "^2.33.1",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-vue": "^6.0.0-beta.11",
		"vue": "^3.0.0"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {}
}
